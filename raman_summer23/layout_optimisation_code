  52\u001b[0m                 \u001b[0;32mif\u001b[0m \u001b[0mdistance\u001b[0m \u001b[0;34m>\u001b[0m \u001b[0mradius1\u001b[0m \u001b[0;34m+\u001b[0m \u001b[0mradius2\u001b[0m\u001b[0;34m:\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n","\u001b[0;31mNameError\u001b[0m: name 'radii' is not defined"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["import numpy as np\n","from scipy.optimize import minimize\n","\n","def objective_function(variables):\n","    H, W, *coordinates = variables\n","    return H * W\n","\n","def adjacency_constraint(variables, k1, k2, radius):\n","    x_k1, y_k1 = variables[2*k1:2*k1+2]\n","    x_k2, y_k2 = variables[2*k2:2*k2+2]\n","    return np.sqrt((x_k1 - x_k2)**2 + (y_k1 - y_k2)**2) - radius[k1] - radius[k2]\n","\n","def boundary_constraint(variables, gamma, radius, side, W, H):\n","    x_gamma, y_gamma = variables[gamma*2:gamma*2+2]\n","    if side == 'right':\n","        return W - (x_gamma + radius[gamma])\n","    elif side == 'top':\n","        return H - (y_gamma + radius[gamma])\n","    elif side == 'bottom':\n","        return y_gamma - radius[gamma]\n","    elif side == 'left':\n","        return x_gamma - radius[gamma]\n","\n","def enclosure_boundaries_constraint(variables, i, radius, W, H):\n","    x_i, y_i = variables[i*2:i*2+2]\n","    return [x_i - radius[i], y_i - radius[i], W - (x_i + radius[i]), H - (y_i + radius[i])]\n","\n","# Given input parameters\n","r = [1, 2, 1]  # Example radius list\n","pairs = [(0, 1), (1, 2)]  # Example adjacency pairs\n","gamma_list = [0, 1, 2]  # Example list of indices\n","\n","# Initial guess\n","initial_guess = [1.0, 1.0, 2.0, 2.0, 3.0, 1.0]\n","\n","# Optimization problem setup\n","constraints = []\n","\n","# Adjacency constraints\n","for k1, k2 in pairs:\n","    constraints.append({'type': 'eq', 'fun': adjacency_constraint, 'args': (k1, k2, r)})\n","\n","# Boundary constraints\n","for gamma in gamma_list:\n","    constraints.append({'type': 'eq', 'fun': boundary_constraint, 'args': (gamma, r, 'right', initial_guess[1], initial_guess[0])})\n","    constraints.append({'type': 'eq', 'fun': boundary_constraint, 'args': (gamma, r, 'top', initial_guess[1], initial_guess[0])})\n","    constraints.append({'type': 'eq', 'fun': boundary_constraint, 'args': (gamma, r, 'bottom', initial_guess[1], initial_guess[0])})\n","    constraints.append({'type': 'eq', 'fun': boundary_constraint, 'args': (gamma, r, 'left', initial_guess[1], initial_guess[0])})\n","\n","# Enclosure boundaries constraints\n","for i in range(len(r)):\n","    constraints.append({'type': 'ineq', 'fun': enclosure_boundaries_constraint, 'args': (i, r, initial_guess[1], initial_guess[0])})\n","\n","# Optimization\n","result = minimize(objective_function, initial_guess, constraints=constraints)\n","\n","# Display the result\n","print(\"Optimal Values:\")\n","print(\"H:\", result.x[0])\n","print(\"W:\", result.x[1])\n","for i in range(len(r)):\n","    print(f\"x_{i}:\", result.x[i*2+2])\n","    print(f\"y_{i}:\", result.x[i*2+3])\n"],"metadata":{"id":"zsnkr4gsKrH0"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["from scipy.optimize import minimize, NonlinearConstraint\n","import matplotlib.pyplot as plt\n","import numpy as np\n","\n","def objective_function(variables):\n","    H, W = variables[0], variables[1]\n","    return H * W\n","\n","def inequality_constraint(variables):\n","    H, W = variables[:2]\n","    x_values = variables[2 : n + 2]\n","    y_values = variables[n + 2: ]\n","    constraints = []\n","\n","    for index in range(n):\n","        constraints += [\n","            r[index] - x_values[index],\n","            x_values[index] - r[index] - W,\n","            -(x_values[index] + r[index]),\n","            x_values[index] + r[index] - W,\n","            y_values[index] - r[index] - H,\n","            r[index] - y_values[index],\n","            y_values[index] + r[index] - H,\n","        ]\n","\n","    for i in range(n):\n","        for j in range(i + 1, n):\n","            if [i, j] not in adjacency and [j, i] not in adjacency:\n","                constraints.append(\n","                    (r[i] + r[j]) ** 2 - (x_values[i] - x_values[j]) ** 2 - (y_values[i] - y_values[j]) ** 2\n","                )\n","\n","    return np.array(constraints)\n","\n","def equality_constraint(variables):\n","    x_values = variables[2 : n + 2]\n","    y_values = variables[n + 2 : ]\n","    constraints = []\n","    for index in gamma:\n","        constraints.append(y_values[index] - r[index])\n","\n","    for k1, k2 in adjacency:\n","        constraints.append(\n","            (x_values[k1] - x_values[k2]) ** 2 + (y_values[k1] - y_values[k2]) ** 2 - (r[k1] + r[k2]) ** 2\n","        )\n","\n","    return np.array(constraints)\n","\n","def plot_circles_and_points(optimal_x, optimal_y, r):\n","    fig, ax = plt.subplots()\n","    for i in range(n):\n","        circle = plt.Circle((optimal_x[i], optimal_y[i]), r[i], fill=False, color='blue')\n","        ax.add_patch(circle)\n","        plt.scatter(optimal_x[i], optimal_y[i], color='red')\n","        plt.text(optimal_x[i] + 0.2, optimal_y[i] + 0.2, str(i), fontsize=10, ha='center', va='center', color='black')\n","\n","    plt.xlabel('X-axis')\n","    plt.ylabel('Y-axis')\n","    plt.title('Optimal Circles and Points')\n","    plt.axis('equal')\n","    plt.show()\n","\n","if __name__ == \"__main__\":\n","    \"\"\"\n","    n = int(input(\"Enter the number of circles: \"))\n","    r, gamma, adjacency = [], [], []\n","\n","    for index in range(n):\n","        r.append(int(input(\"Enter the radii: \")))\n","\n","    gammas = int(input(\"Enter the number of gammas: \"))\n","    for index in range(gammas):\n","        gamma.append(int(input(\"Enter the value of gamma: \")))\n","\n","    adjacencies = int(input(\"Enter the number of adjacency constraints: \"))\n","    for index in range(adjacencies):\n","        k1, k2 = map(int, input(\"Enter the values of k1 and k2: \").split())\n","        adjacency.append([k1, k2])\n","    \"\"\"\n","    n = 7\n","    r = [1, 3, 5, 7, 9, 2, 6]\n","    gammas, adjacencies = 4, 2\n","    gamma = [1, 5, 6]\n","    adjacencies = [[1, 2], [4, 6]]\n","\n","    initial_guess = [1.0, 1.0] + [0.0] * (2 * n)\n","    bounds = [(0, None)] * 2 + [(0, None)] * (2 * n)\n","\n","    constraints = [\n","        NonlinearConstraint(fun=inequality_constraint, lb=-np.inf, ub=0),\n","        NonlinearConstraint(fun=equality_constraint, lb=-0.90, ub=0.90)\n","    ]\n","\n","    result = minimize(\n","        fun=lambda variables: objective_function(variables),\n","        x0=initial_guess,\n","        bounds=bounds,\n","        constraints=constraints,\n","        method='trust-constr'\n","    )\n","\n","    optimal_H, optimal_W, optimal_x, optimal_y = result.x[0], result.x[1], result.x[2 : n + 2], result.x[n + 2 :]\n","\n","    print(\"Optimal values: \", optimal_H, optimal_W, optimal_x, optimal_y)\n","    plot_circles_and_points(optimal_x, optimal_y, r)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":577},"id":"xRVCSi6qIrPf","executionInfo":{"status":"ok","timestamp":1702386653223,"user_tz":-330,"elapsed":4920,"user":{"displayName":"Om Singh","userId":"02223458389634913969"}},"outputId":"d38ffb46-fab9-4689-889b-9b488efeb8f5"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stderr","text":["/usr/local/lib/python3.10/dist-packages/scipy/optimize/_hessian_update_strategy.py:182: UserWarning: delta_grad == 0.0. Check if the approximated function is linear. If the function is linear better results can be obtained by defining the Hessian as zero instead of using quasi-Newton approximations.\n","  warn('delta_grad == 0.0. Check if the approximated '\n"]},{"output_type":"stream","name":"stdout","text":["Optimal values:  18.000007949059917 50.537611378997916 [35.20366747 32.44855268 32.1822575