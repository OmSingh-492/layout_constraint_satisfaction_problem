ay(constraints)\n","\n","def plot_circles_and_points(optimal_x, optimal_y, r):\n","    fig, ax = plt.subplots()\n","    for i in range(n):\n","        circle = plt.Circle((optimal_x[i], optimal_y[i]), r[i], fill=False, color='blue')\n","        ax.add_patch(circle)\n","        plt.scatter(optimal_x[i], optimal_y[i], color='red')\n","        plt.text(optimal_x[i] + 0.2, optimal_y[i] + 0.2, str(i), fontsize=10, ha='center', va='center', color='black')\n","\n","    plt.xlabel('X-axis')\n","    plt.ylabel('Y-axis')\n","    plt.title('Optimal Circles and Points')\n","    plt.axis('equal')\n","    plt.show()\n","\n","def generate_inputs():\n","    # mapping[i]: String corresponding to index i.\n","    mapping, index_val = dict(), dict()\n","    # index_val[i]: Index corresponding to the string i.\n","    count = 0\n","    r, gamma, adjacency = [], [], []\n","\n","    with open(\"sizes.txt\", \"r\") as file:\n","        for line in file:\n","            str_value, size_value = line.strip().split(\",\")\n","            mapping[count] = str_value\n","            index_val[str_value] = count\n","            r.append(float(size_value))\n","    n = count + 1\n","    count = 0\n","    with open(\"anchors.txt\", \"r\") as file:\n","        for line in file:\n","            gamma.append(index_val[line.strip()])\n","            count += 1\n","    gammas = count\n","    count = 0\n","    with open(\"edges.txt\", \"r\") as file:\n","        for line in file:\n","            str1, str2 = line.strip().split(\",\")\n","            k1, k2 = index_val[str1], index_val[str2]\n","            adjacency.append([k1, k2])\n","            count += 1\n","    adjacencies = count\n","    return n, gammas, adjacencies, r, gamma, adjacency\n","\n","if __name__ == \"__main__\":\n","    n, gammas, adjacencies, r, gamma, adjacency = generate_inputs()\n","\n","    initial_guess = [1.0, 1.0] + [0.0] * (2 * n)\n","    bounds = [(0, None)] * 2 + [(0, None)] * (2 * n)\n","\n","    constraints = [\n","        NonlinearConstraint(fun=inequality_constraint, lb=-np.inf, ub=0),\n","        NonlinearConstraint(fun=equality_constraint, lb=-0.90, ub=0.90)\n","    ]\n","\n","    result = minimize(\n","        fun=lambda variables: objective_function(variables),\n","        x0=initial_guess,\n","        bounds=bounds,\n","        constraints=constraints,\n","        method='trust-constr'\n","    )\n","\n","    optimal_H, optimal_W, optimal_x, optimal_y = result.x[0], result.x[1], result.x[2 : n + 2], result.x[n + 2 :]\n","\n","    print(\"Optimal values: \", optimal_H, optimal_W, optimal_x, optimal_y)\n","    plot_circles_and_points(optimal_x, optimal_y, r)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":361},"id":"1sPhP05G1SpA","executionInfo":{"status":"error","timestamp":1702465230782,"user_tz":-330,"elapsed":1964,"user":{"displayName":"Om Singh","userId":"02223458389634913969"}},"outputId":"354fe365-bfd4-4f36-d401-3cc1aee4b91e"},"execution_count":1,"outputs":[{"output_type":"error","ename":"FileNotFoundError","evalue":"ignored","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mFileNotFoundError\u001b[0m                         Traceback (most recent call last)","\u001b[0;32m<ipython-input-1-1a30bf49f723>\u001b[0m in \u001b[0;36m<cell line: 92>\u001b[0;34m()\u001b[0m\n\u001b[1;32m     91\u001b[0m \u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m     92\u001b[0m \u001b[0;32mif\u001b[0m \u001b[0m__name__\u001b[0m \u001b[0;34m==\u001b[0m \u001b[0;34m\"__main__\"\u001b[0m\u001b[0;34m:\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0;32m---> 93\u001b[0;31m     \u001b[0mn\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mgammas\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0madjacencies\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mr\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mgamma\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0madjacency\u001b[0m \u001b[0;34m=\u001b[0m \u001b[0mgenerate_inputs\u001b[0m\u0